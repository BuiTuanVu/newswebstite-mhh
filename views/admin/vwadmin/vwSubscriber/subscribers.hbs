        })

        $('button#btnView').click(function () {
            $("#table-id").slideUp('fast', function () {
                $("#view-form").slideDown('fast');
            })
        })

        $('#edit-cancel').click(function () {
            $("#edit-form").slideUp('fast', function () {
                $("#table-id").slideDown('fast');
            })
        })

        $('#cancel-view').click(function () {
            $("#view-form").slideUp('fast', function () {
                $("#table-id").slideDown('fast');
            })
        })

        $('#main-navbar').dynamicMenu();

        $('#child-two').dynamicMenu();

        if ($('#child-two li').hasClass('active')) {
            $('#child-two').addClass('menu-open').css({ 'display': 'block' });
        }
    })

